{
  "extends": "../jsconfig-base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "bangle-io-context": [
        "../bangle-io-context/index.js"
      ],
      "bangle-io-context/*": [
        "../bangle-io-context/*"
      ],
      "collapsible-heading": [
        "../extensions/collapsible-heading/index.js"
      ],
      "collapsible-heading/*": [
        "../extensions/collapsible-heading/*"
      ],
      "editor": [
        "../editor/index.js"
      ],
      "editor/*": [
        "../editor/*"
      ],
      "image-extension": [
        "../extensions/image-extension/index.js"
      ],
      "image-extension/*": [
        "../extensions/image-extension/*"
      ],
      "inline-backlink": [
        "../extensions/inline-backlink/index.js"
      ],
      "inline-backlink/*": [
        "../extensions/inline-backlink/*"
      ],
      "inline-command-palette": [
        "../extensions/inline-command-palette/index.js"
      ],
      "inline-command-palette/*": [
        "../extensions/inline-command-palette/*"
      ],
      "inline-emoji": [
        "../extensions/inline-emoji/index.js"
      ],
      "inline-emoji/*": [
        "../extensions/inline-emoji/*"
      ]
    }
  }
}