{
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "prettier.prettierPath": ".yarn/sdks/prettier/index.js",
  "cSpell.words": [
    "dhancha",
    "naukar",
    "Unmount",
    "worktree",
    "Worktrees"
  ],
  "javascript.preferences.importModuleSpecifier": "project-relative",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#bfdba3",
    "activityBar.activeBorder": "#5f8fc0",
    "activityBar.background": "#bfdba3",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#5f8fc0",
    "activityBarBadge.foreground": "#15202b",
    "editorGroup.border": "#bfdba3",
    "panel.border": "#bfdba3",
    "sash.hoverBorder": "#bfdba3",
    "sideBar.border": "#bfdba3",
    "statusBar.background": "#a5cc7f",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#8bbd5b",
    "statusBarItem.remoteBackground": "#a5cc7f",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#a5cc7f",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#a5cc7f99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#a5cc7f"
}