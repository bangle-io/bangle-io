{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "react",
    "paths": {
      "app/*": [
        "./app/*"
      ],
      "app": [
        "./app/index.js"
      ],
      "utils/*": [
        "./utils/*"
      ],
      "utils": [
        "./utils/index.js"
      ],
      "config/*": [
        "./config/*"
      ],
      "config": [
        "./config/index.js"
      ],
      "style/*": [
        "./style/*"
      ],
      "style": [
        "./style/index.js"
      ],
      "editor/*": [
        "./editor/*"
      ],
      "editor": [
        "./editor/index.js"
      ],
      "ui-context": [
        "./ui-context/index.js"
      ],
      "ui-context/*": [
        "./ui-context/*"
      ],
      "ui-components": [
        "./ui-components/index.js"
      ],
      "ui-components/*": [
        "./ui-components/*"
      ],
      "workspace": [
        "./workspace/index.js"
      ],
      "workspace/*": [
        "./workspace/*"
      ],
      "bangle-io-context": [
        "./bangle-io-context/index.js"
      ],
      "bangle-io-context/*": [
        "./bangle-io-context/*"
      ],
      "baby-fs": [
        "./js-libs/baby-fs/index.js"
      ],
      "baby-fs/*": [
        "./js-libs/baby-fs/*"
      ],
      "inline-palette": [
        "./js-libs/inline-palette/index.js"
      ],
      "inline-palette/*": [
        "./js-libs/inline-palette/*"
      ]
    },
    "module": "esnext",
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "archive",
    "public",
    ".cache",
    ".yarn",
    "coverage",
    ".pnp.js"
  ]
}